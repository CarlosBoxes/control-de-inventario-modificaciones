	<queryString>
		<![CDATA[SELECT
     SUM(descripcionpedido.`subTotal`) AS descripcionpedido_subTotal,
     SUM(descripcionpedido.`Cantidad`) AS descripcionpedido_Cantidad,
     pedido.`fecha` AS pedido_fecha,
     pedido.`observaciones` AS pedido_observaciones,
     pedido.`idpedido` AS pedido_idpedido,
     pedido.`liquidado` AS pedido_liquidado,
     productos.`idProductos` AS productos_idProductos,
     productos.`nombre` AS productos_nombre,
     productos.`presentacion` AS productos_presentacion,
     productos.`precio_costo` AS Productos_precio_costo,
     bodegaproductos.`idbodega` AS bodegaproductos_idbodega,
     bodegaproductos.`nombre` AS bodegaproductos_nombre
FROM
     `pedido` pedido INNER JOIN `descripcionpedido` descripcionpedido ON pedido.`idpedido` = descripcionpedido.`pedido_idpedido`
     INNER JOIN `productos` productos ON descripcionpedido.`Productos_idProductos` = productos.`idProductos`
     INNER JOIN `inventario producto` inventario_producto ON productos.`idProductos` = inventario_producto.`Productos_idProductos`
     INNER JOIN `bodegaproductos` bodegaproductos ON inventario_producto.`bodegaProductos_idbodega` = bodegaproductos.`idbodega`
WHERE
     bodegaproductos_idbodega = $P{ID}
 AND pedido.liquidado = true
 AND YEAR(pedido.fecha) = $P{Año}
 AND MONTH(pedido.fecha) = $P{Mes}
GROUP BY
     productos_nombre,
     productos_presentacion]]>
	</queryString>
