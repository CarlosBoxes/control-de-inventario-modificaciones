          <image>
            <Image url="@../Imagenes/Logo.jpg" preserveRatio="false" smooth="false" />
          </image>
        </ImageView>
      </center>
      <left>
        <VBox fx:id="pnlMenu" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="180.0" spacing="20.0" styleClass="PanelMenu">
          <children>
            <Button fx:id="btnPedidos" alignment="CENTER" mnemonicParsing="false" onAction="#abrirPedidos" prefHeight="60.0" prefWidth="180.0" styleClass="botonPedidos" text="Pedidos" />
            <Button fx:id="btnBodegas" mnemonicParsing="false" onAction="#abrirBodegas" prefHeight="60.0" prefWidth="180.0" styleClass="botonBodega" text="Bodegas" />
            <Button fx:id="btnProduccion" alignment="CENTER" mnemonicParsing="false" onAction="#abrirProduccion" prefHeight="60.0" prefWidth="180.0" styleClass="botonProduccion" text="ProducciÃ³n" />
            <Button fx:id="btnLiquidaciones" mnemonicParsing="false" onAction="#abrirLiquidaciones" prefHeight="60.0" prefWidth="180.0" styleClass="botonLiquidaciones" text="Liquidaciones" />
            <Button fx:id="btnFinanzas" mnemonicParsing="false" onAction="#abrirFinanzas" prefHeight="60.0" prefWidth="180.0" styleClass="botonFinanzas" text="Finanzas" />
            <Button fx:id="btnOpciones" mnemonicParsing="false" onAction="#abrirOpciones" prefHeight="60.0" prefWidth="180.0" styleClass="botonOpciones" text="Opciones" />
            <Button fx:id="btnSalir" mnemonicParsing="false" onAction="#cerrarAplicacion" prefHeight="60.0" prefWidth="180.0" styleClass="botonSalir" text="Salir" />
          </children>
        </VBox>
      </left>
    </BorderPane>
  </children>
  <stylesheets>
    <URL value="@estilos.css" />
  </stylesheets>
